<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="bpms" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressDate" value="false"/>
            <property name="suppressAllComments" value="false"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/bpms?serverTimezone=Asia/Shanghai&amp;AllowPublicKeyRetrieval=True"
                        userId="root" password="root">
        </jdbcConnection>
        <javaModelGenerator targetPackage="com.zdxr.cc.mgr.sl.entity" targetProject="src/main/java"/>
        <sqlMapGenerator targetPackage="generator.mapping" targetProject="src/main/resources"/>
        <javaClientGenerator targetPackage="com.zdxr.cc.mgr.sl.mapper" targetProject="src/main/java"
                             type="XMLMAPPER"/>
        <table schema="bpms" tableName="bpms_quality">
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>
    </context>
</generatorConfiguration>